{
  "compilerOptions": {
    "lib": [
      "ES5",
      "ES6",
    ],
    "target": "ES5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "types": ["node", "typeorm"]
  },
  "include": [
    "./src/**/*.ts",
    "migrations/**/*.ts"
  ]
}